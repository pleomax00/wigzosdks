<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wigzo.sdk">

    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <permission android:name="wigzo.sdk.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="wigzo.sdk.permission.C2D_MESSAGE" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <service
            android:name="wigzo.sdk.WigzoGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <service
            android:name="wigzo.sdk.WigzoInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>

    </application>

</manifest>
